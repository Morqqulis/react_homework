{
	"singleQuote": true,
	"tabWidth": 4,
	"semi": false,
	"trailingComma": "none",
	"printWidth": 120,
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"jsxSingleQuote": true,
	"overrides": [
		{
			"files": "*.json",
			"options": {
				"printWidth": 80
			}
		}
	],

	"plugins": ["prettier-plugin-tailwindcss"]
}
